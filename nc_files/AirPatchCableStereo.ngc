(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 1 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)

G01 X #<slot1Partx> y #<slot1Party>	( go to slot 1 part position 1 )

G91	( relative coordinates )

G00 X #<dragHolex> Y #<dragHoley>

G90	( absolute coordinates )

G01 Z #<slot1DragZ>	( go down into the drag hole )

G91	( relative coordinates )

G01 F50 Y -#<dragHoley>	( drag it two slots to present a new component )

G01 F50 Y -#<dragHoley>

G01 F50 Y -#<radDiff>

G01 F50 Y #<radDiff>	( move back into the center of the drag hole )

G90	( absolute coordinates )

G00 Z #<safeZ>

(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 1 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)





(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 2 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)

G01 X #<slot2Partx> y #<slot2Party>	( go to slot 1 part position 1 )

G91	( relative coordinates )

G00 X #<dragHolex> Y #<dragHoley>

G90	( absolute coordinates )

G01 Z #<slot2DragZ>	( go down into the drag hole )

G91	( relative coordinates )

G01 F50 Y -#<dragHoley>	( drag it two slots to present a new component )

G01 F50 Y -#<dragHoley>

G01 F50 Y -#<radDiff>

G01 F50 Y #<radDiff>	( move back into the center of the drag hole )

G90	( absolute coordinates )

G00 Z #<safeZ>

(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 2 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)





(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 5 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)

G01 X #<slot5Partx> y #<slot5Party>	( go to slot 1 part position 1 )

G91	( relative coordinates )

G00 X #<dragHolex> Y #<dragHoley>

G90	( absolute coordinates )

G01 Z #<slot5DragZ>	( go down into the drag hole )

G91	( relative coordinates )

G01 F50 Y -#<dragHoley>	( drag it two slots to present a new component )

G01 F50 Y -#<dragHoley>

G01 F50 Y -#<radDiff>

G01 F50 Y #<radDiff>	( move back into the center of the drag hole )

G90	( absolute coordinates )

G00 Z #<safeZ>

(~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Feed Slot 5 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~)

G00 X -4.000 Y 0.000

M30

